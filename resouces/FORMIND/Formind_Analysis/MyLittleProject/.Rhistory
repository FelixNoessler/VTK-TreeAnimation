calc_BA<-function(D,N,Nr_hectare){
if(length(N)==0){
return(0)
}else{
return(sum((D/2)^2*pi*N)/Nr_hectare)
}
}
calc_height_heterogeneity<-function(H,N){
if(length(N)<=1){
return(0)
}else{
return(sd(rep(H,N),na.rm=T))
}
}
